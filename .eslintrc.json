{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "allowImportExportEverywhere": true,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": 0,
        "no-var": 0,
        "no-redeclare": 0,
        "no-path-concat": 0,
        "no-unused-expressions": 0
    },

    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}